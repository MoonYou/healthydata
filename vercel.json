{
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/index.html"
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend.com/api/$1"
    }
  ]
}
